						</CatalogSourceId>
						<Description>
							<AStr
								ws="en">
								<Run
									ws="en">Singular number is number that refers to one member of a designated class.</Run>
							</AStr>
						</Description>
						<Name>
							<AUni
								ws="en">singular number</AUni>
						</Name>
						<ShowInGloss
							val="true" />
