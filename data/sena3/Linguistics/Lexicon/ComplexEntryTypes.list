							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="31de15dc-131a-44e9-a223-ae174e4091f6">
								<Contents>
									<Str>
										<Run
											ws="en">Example (Akan)</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="d26793fc-520f-4826-b548-c33cc86d9a46">
								<Contents>
									<Str>
										<Run
											ws="en">        The item gyee ... so 'answered' is a phrasal verb, as in the following:</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="0c5d9181-f128-4f24-adfa-95aad8143dfa">
								<Contents>
									<Str>
										<Run
											ws="en">        Kofi gyee Kwame so</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
