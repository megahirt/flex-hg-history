				<Description>
					<AStr
						ws="en">
						<Run
							ws="en">A stem that is made up of a root plus an affix that adds a non-inflectional component of meaning.</Run>
					</AStr>
				</Description>
				<Discussion>
					<StText
						guid="8957de3a-c92d-4bcc-a226-3f3ff1c955c2">
						<DateModified
							val="2011-2-2 19:19:27.744" />
						<Paragraphs>
							<ownseq
								class="StTxtPara"
								guid="85ac0f39-194b-4841-8ba9-950e9036a360">
								<Contents>
									<Str>
										<Run
											ws="en">The derived word is often of a different word class from the original.  It may thus take the inflectional affixes of the new word class.  In contrast to inflection, derivation</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="52fdc1ea-a41b-4724-b3c7-a0027f71e5f9">
								<Contents>
									<Str>
										<Run
											ws="en">        is not obligatory</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="dec1a454-d80f-49dd-ab05-6b08ff7f54b4">
								<Contents>
									<Str>
										<Run
											ws="en">        typically produces a greater change of meaning from the original form</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="201530a9-b60d-441d-9803-2bf908804e39">
								<Contents>
									<Str>
										<Run
											ws="en">        is more likely to result in a form which has a somewhat idiosyncratic meaning, and</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="59c8e9c0-934c-4255-9762-3be5cf81c9b8">
								<Contents>
									<Str>
										<Run
											ws="en">        often changes the grammatical category of a root.</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="42598f7e-ae10-4c2d-bf38-ece3e10364ab">
								<Contents>
									<Str>
										<Run
											ws="en">Derivational operations</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="8603cac4-292a-4f38-ad36-c61d8fe31603">
								<Contents>
									<Str>
										<Run
											ws="en">        tend to be idiosyncratic and non-productive</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="dd5cbaeb-8e6e-4106-b2cb-e19696cfd46d">
								<Contents>
									<Str>
										<Run
											ws="en">        do not occur in well-defined 'paradigms,' and</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="16d7129c-9fbe-4399-9667-717e8584641c">
								<Contents>
									<Str>
										<Run
											ws="en">        are 'optional' insofar as they</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="c8e13ad6-41c4-4aa9-ab2b-845f57f1ca45">
								<Contents>
									<Str>
										<Run
											ws="en">                shape the basic semantic content of roots and</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="c2b0b9b7-a7be-4582-aad0-55d62faf2551">
								<Contents>
									<Str>
										<Run
											ws="en">                are not governed by some other syntactic operation or element.</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="32e15999-c949-44f4-8852-a2ece639f85b">
								<Contents>
									<Str>
										<Run
											ws="en">Examples (English)</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="b011ce84-5ba1-48a7-a7d1-3fc8ed1a6bbc">
								<Contents>
									<Str>
										<Run
											ws="en">        Kindness is derived from kind.</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="d1dd380f-5177-4b77-83f8-304b18640c74">
								<Contents>
									<Str>
										<Run
											ws="en">        Joyful is derived from joy.</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="476b2eeb-e97b-4091-a7ee-5e972b1f2934">
								<Contents>
									<Str>
										<Run
											ws="en">        Amazement is derived from amaze.</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="35fe9870-c304-4b41-a03d-9a1c6392ebca">
								<Contents>
									<Str>
										<Run
											ws="en">        Speaker is derived from speak.</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="f28efe46-651e-4bc5-a7f6-a245f2095e19">
								<Contents>
									<Str>
										<Run
											ws="en">        National is derived from nation.</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="004867eb-727a-4aa9-8c53-6f101b5608df">
								<Contents>
									<Str>
										<Run
											ws="en">Kinds</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="0e6fbfa6-19c9-425b-b1cf-cca7d44a321b">
								<Contents>
									<Str>
										<Run
											ws="en">Here are some kinds of derivational operations:</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="3354c363-7764-413a-b382-39fafbcaebfe">
								<Contents>
									<Str>
										<Run
											ws="en">        Operations that change the grammatical category of a root</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="1baad9d0-63bd-4355-8a74-2c7c22ddf76b">
								<Contents>
									<Str>
										<Run
											ws="en">                Example: Nominalization (English)</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="d51a595f-0e36-4468-b3ef-6274236d2173">
								<Contents>
									<Str>
										<Run
											ws="en">                Verbs and adjectives can be turned into nouns: amaze &gt; amazement, speak &gt; speaker, perform &gt; performance, soft &gt; softness, warm &gt; warmth</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="cd711149-7d3e-4c18-b2e5-ed076319ff10">
								<Contents>
									<Str>
										<Run
											ws="en">        Operations that change the valence (transitivity) of a root</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="85ae8f9c-0afd-43c0-bd35-c3fff4469f31">
								<Contents>
									<Str>
										<Run
											ws="en">                Example: Causation (Swahili)</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="54f97360-5138-40ed-bd6a-e1f427216507">
								<Contents>
									<Str>
										<Run
											ws="en">                kula 'to eat' &gt; kulisha, 'to feed'</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
