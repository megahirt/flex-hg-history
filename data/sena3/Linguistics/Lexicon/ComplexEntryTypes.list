								<Contents>
									<Str>
										<Run
											ws="en">Example (English)</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="4df6f1c1-063d-4acc-9314-05c7ef3998af">
								<Contents>
									<Str>
										<Run
											ws="en">         The item give up is a phrasal verb, as in the following:</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="8aec424f-e2f7-4b21-81b7-86a60cb1314f">
								<Contents>
									<Str>
										<Run
											ws="en">        He gave up smoking.</Run>
									</Str>
								</Contents>
								<ParseIsCurrent
									val="False" />
							</ownseq>
							<ownseq
								class="StTxtPara"
								guid="1a887665-55f2-43be-93e4-6e54dbf9a9b3">
								<Contents>
