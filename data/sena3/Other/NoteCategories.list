								<ForeColor
									val="-1073741824" />
								<Hidden
									val="False" />
								<IsProtected
									val="False" />
								<Name>
									<AUni
										ws="en">Gender appropriate</AUni>
								</Name>
								<SortSpec
									val="0" />
								<UnderColor
									val="-1073741824" />
